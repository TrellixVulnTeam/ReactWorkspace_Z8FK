{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387

  // Note: remember to add pythonPath to the
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Todo - Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/todo"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "python": "${workspaceFolder}/djangoAdvanture/python3/bin/python",
      "program": "${workspaceFolder}/djangoAdvanture/src/manage.py",
      "args": ["runserver"],
      "django": true
    }
  ]
}
